{
    "name": "VectorNav-PIO",
    "version": "0.0.0",
    "description": "VectorNav Sensors SDK for PlatformIO",
    "keywords": "vectornav, imu, sensor, navigation",
    "repository": {
      "type": "git",
      "url": "https://github.com/sgrsn/VectorNav-PIO.git"
    },
    "authors": [
      {
        "name": "Hidaka SATO",
        "email": "sgrsn1711@gmail.com",
        "maintainer": true
      }
    ],
    "license": "MIT",
    "frameworks": "*",
    "platforms": "*",
    "build": {
      "includeDir": "include",
      "unflags": "-std=gnu++11",
      "flags": [
        "-DTHREADING_ENABLE=false",
        "-DPUGIXML_NO_EXCEPTIONS",
        "-std=gnu++17"
      ]
    },
    "dependencies": [
      {
        "name": "TeensyThreads",
        "version": "https://github.com/ftrias/TeensyThreads.git",
        "platforms": "teensy"
      }
    ]
  }